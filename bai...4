#include <iostream>
using namespace std;

int X(int n) {
    if (n == 0) {
        return 1;
    } else {
        int sum = 0;
        for (int i = 0; i <= n-2; i++) {
            sum += (n-i)*(n-i)*X(i);
        }
        sum += X(n-1);
        return sum;
    }
}

int main() {
    int n;
    cout << "Enter n: ";
    cin >> n;
    int xn = X(n);
    cout << "X(" << n << ") = " << xn << endl;
    return 0;
}
